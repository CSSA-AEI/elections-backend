{
  "name": "elections-backend",
  "version": "1.0.0",
  "private": true,
  "main": "server.ts",
  "repository": "git@github.com:cssa-aei/elections-backend.git",
  "author": "it@cssa-aei.ca",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn run watch:build\" \"yarn run watch:server\"",
    "watch:build": "tsc --watch",
    "watch:server": "NODE_ENV=test nodemon ./dist/server.js --watch ./dist",
    "clean": "rimraf dist",
    "build": "tsc",
    "postinstall": "tsc",
    "start": "node dist/server.js",
    "populate": "tsc && node dist/src/scripts/populate.js",
    "lint": "eslint . --ext .ts --fix",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "engines": {
    "node": "^16.9.1",
    "yarn": "^1.22.10"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "http": "^0.0.1-security",
    "mongoose": "^6.0.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.1",
    "@types/mailgun-js": "^0.22.11",
    "@types/mongoose": "^5.10.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mailgun-js": "^0.22.0",
    "nodemon": "^2.0.6",
    "pac-resolver": "5.0.0",
    "prettier": "^2.2.1",
    "read-excel-file": "^5.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.1.3"
  }
}
